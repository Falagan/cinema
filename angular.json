{
  "version": 1,
  "projects": {
    "cinema-web": {
      "projectType": "application",
      "root": "apps/cinema-web",
      "sourceRoot": "apps/cinema-web/src",
      "prefix": "cinema",
      "schematics": {
        "@schematics/angular:component": {
          "changeDetection": "OnPush"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/cinema-web",
            "index": "apps/cinema-web/src/index.html",
            "main": "apps/cinema-web/src/main.ts",
            "polyfills": "apps/cinema-web/src/polyfills.ts",
            "tsConfig": "apps/cinema-web/tsconfig.app.json",
            "assets": ["apps/cinema-web/src/favicon.ico", "apps/cinema-web/src/assets"],
            "styles": [
              "apps/cinema-web/src/styles.css",
              "node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/cinema-web/src/environments/environment.ts",
                  "with": "apps/cinema-web/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "preproduction": {
              "fileReplacements": [
                {
                  "replace": "apps/cinema-web/src/environments/environment.ts",
                  "with": "apps/cinema-web/src/environments/environment.pre.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "apps/cinema-web/src/environments/environment.ts",
                  "with": "apps/cinema-web/src/environments/environment.dev.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "cinema-web:build:production"
            },
            "preproduction": {
              "browserTarget": "cinema-web:build:preproduction"
            },
            "development": {
              "browserTarget": "cinema-web:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "cinema-web:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/cinema-web/src/**/*.ts", "apps/cinema-web/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/cinema-web"],
          "options": {
            "jestConfig": "apps/cinema-web/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "cinema-web-e2e": {
      "root": "apps/cinema-web-e2e",
      "sourceRoot": "apps/cinema-web-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/cinema-web-e2e/cypress.json",
            "devServerTarget": "cinema-web:serve:development",
            "tsConfig": "apps/cinema-web-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "cinema-web:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/cinema-web-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["cinema-web"]
    },
    "lib-cinema": {
      "root": "libs/lib-cinema",
      "sourceRoot": "libs/lib-cinema/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/lib-cinema/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/lib-cinema"],
          "options": {
            "jestConfig": "libs/lib-cinema/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "lib-store": {
      "root": "libs/lib-store",
      "sourceRoot": "libs/lib-store/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/lib-store/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/lib-store"],
          "options": {
            "jestConfig": "libs/lib-store/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    }
  }
}
